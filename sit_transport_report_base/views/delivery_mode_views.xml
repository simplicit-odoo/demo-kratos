<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_delivery_carrier_mode_tree" model="ir.ui.view">
        <field name="name">delivery.carrier.mode.tree</field>
        <field name="model">delivery.carrier.mode</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="action_delivery_carrier_mode_list" model="ir.actions.act_window">
        <field name="name">Delivery carrier mode</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">delivery.carrier.mode</field>
        <field name="view_mode">tree</field>
        <!-- <field name="context">{}</field> -->
    </record>

    <menuitem action="action_delivery_carrier_mode_list" id="menu_action_delivery_carrier_mode_list" parent="delivery.menu_delivery" sequence="5"/>
</odoo>
